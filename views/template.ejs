<script>
        
    const bar2 = document.querySelector('.ldBar');
    const assets = document.getElementsByClassName("asset");
    const b = new ldBar(bar2);
    const interval = setInterval(checkLoading, 20);
    let perc = 0;
    let assetCounter = 0;

    for (let i = 0; i < assets.length; i++) {
        assets[i].addEventListener("load", function () {
            assetCounter++;
        });
    };

    function checkLoading() {

        perc = (assetCounter * 100) / assets.length;
        b.set(perc);
        console.log(perc);
        if(assetCounter === assets.length){
            clearInterval(interval);
            setTimeout(function() {
                let preloader = document.getElementsByClassName("preloader");
                let container = document.getElementsByClassName("container-home");
                preloader[0].style.opacity = 0;
                preloader[0].style.display = "none"; 
                            
                setTimeout(function() {
                    container[0].style.opacity=1;
                    container[0].style.display = "block";
                    }, 200);
                }, 100);           
        };
    };


</script>
